#include<iostream>

using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int m,n;
        cin>>m>>n;
        int l=(m+1)+(m*2);
         int c=(n+1)+(n*2);
       
        for(int i=0;i<m;i++)
        {
            for(int j=0;j<n;j++)
            {
                if(i%3==0||j%3==0)
                cout<<"*";
                
                else cout<<".";
            }
            cout<<endl;

        }   
    
    }
    return 0;  
