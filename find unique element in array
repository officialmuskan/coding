#include<iostream>
using namespace std;

void input(int a[],int used_size)
{
    cout<<"Enter the elements in an array :";
    for(int i=0;i<used_size;i++)
    {
        cin>>a[i];
    }

}


//Unique element in array
void unique(int a[],int used_size)
{
    int x0r;
    for(int i=0;i<used_size;i++)
    {
        x0r^=a[i];
    }
    cout<<x0r;
}
int main()
{
    int a[7]={1,2,4,6,2,4,1};
    unique(a,7);
    // int a[10],used_size;
    // cout<<"Enter the size of an array : ";
    // cin>>used_size;
    // input(a,used_size);
    // unique(a,used_size);
    return 0;
}






//XOR PROPERTIES
// a^a=0
// a^0=a
