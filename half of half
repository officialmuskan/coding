// half of half
#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t)
    {
        string s,s1;
        cin>>s;
        for(int i=0;i<s.length()/2;i=i+2)
        {
           s1=s1+s[i];
        }
           cout<<s1;    
    }
    return 0;    
}
//output
your
y
progress
po
